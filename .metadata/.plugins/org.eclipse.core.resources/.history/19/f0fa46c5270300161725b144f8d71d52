#include <gtest/gtest.h>
#include <gmock/gmock.h>

#define UNIT_TEST

#include "FunctionTracer.h"
using namespace std;
using ::testing::_;
using ::testing::Return;
using ::testing::Invoke;
using ::testing::TestWithParam;
using ::testing::Values;
using ::testing::StrCaseEq;

TEST(House, TraceLevel0_inactive) {
   //FUNCTION_TRACE
   unique_ptr<FunctionTracer> p;
   int ThreadId = getpid();
   int Counter = FunctionTraceCounter.get(ThreadId);
   if (Counter < FunctionTrace_TraceLevel) {
      p = unique_ptr < FunctionTracer > (new FunctionTracer(__FUNCTION__));
   }
   ASSERT_EQ(p->EntryTraceDone, false);
}
