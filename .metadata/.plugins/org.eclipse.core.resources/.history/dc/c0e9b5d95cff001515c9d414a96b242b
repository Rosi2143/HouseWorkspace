#include "FunctionTace.h"

void test3()
{
   FUNCTION_TRACE
   std::cout << "test3 Ende" << std::endl;
   usleep(3000);
}

void test2()
{
   FUNCTION_TRACE
   test3();
   usleep(2000);
   std::cout << "test2 Ende" << std::endl;
}

void test1()
{
   FUNCTION_TRACE
   test2();
   usleep(1000);
   std::cout << "test Ende" << std::endl;
}
