#ifndef ROOM_H
#define ROOM_H

#include <string>
#include <list>

struct Room {
      std::string Name;
      std::vector<unsigned int> Lights;
      std::vector<unsigned int> Heaters;
      std::vector<unsigned int> Blinds;
      std::vector<Switch>       Switches;
};

typedef std::vector<Room> Rooms;

#endif /* ROOM_H */
