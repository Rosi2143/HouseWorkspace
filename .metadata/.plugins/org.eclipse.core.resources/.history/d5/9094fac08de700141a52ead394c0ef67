#include <boost/statechart/state_machine.hpp>
#include <boost/statechart/simple_state.hpp>
#include <iostream>

namespace sc = boost::statechart;

struct BlindSomewhere;

struct Machine : sc::state_machine< Machine, BlindSomewhere > {};
